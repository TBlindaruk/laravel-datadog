{
    "name": "airslate/laravel-datadog",
    "description": "AirSlate internal API client library.",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Roman Vitruk",
            "email": "skreib@gmail.com",
            "role": "PHP developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "datadog/php-datadogstatsd": "1.3.*",
        "illuminate/routing": "^5.6",
        "illuminate/support": "^5.6",
        "illuminate/events": "^5.6"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "AirSlate\\Datadog\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AirSlate\\Datadog\\ServiceProviders\\DataDogProvider",
                "AirSlate\\Datadog\\ServiceProviders\\EventServiceProvider"
            ]
        }
    }
}
